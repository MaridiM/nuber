{
  "name": "nuber_server",
  "version": "1.0.0",
  "description": "Server for (N)Uber Clone, GraphQL, Typescript, NodeJS, PostgreSQL",
  "main": "index.js",
  "scripts": {
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-codegen"
  },
  "keywords": [
    "react",
    "node",
    "typescript",
    "express",
    "postgresql",
    "graphql",
    "apolo"
  ],
  "author": "MaridiM <maridim.dev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.3.2",
    "@graphql-tools/merge": "^6.2.11",
    "@graphql-tools/schema": "^7.1.3",
    "apollo-datasource": "^0.8.0",
    "apollo-server-express": "^2.22.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.37",
    "babel-runtime": "^6.26.0",
    "concurrently": "^6.0.0",
    "gql-merge": "0.0.6",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  }
}
